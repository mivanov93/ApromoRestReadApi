<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Applications" table="applications">
    <id name="applId" type="integer" column="appl_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="applEmail" type="string" column="appl_email" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="applUrl" type="string" column="appl_url" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="applPerson" type="string" column="appl_person" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="applCompany" type="string" column="appl_company" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="applEik" type="string" column="appl_eik" length="16" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="applRegion" type="string" column="appl_region" length="32" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="applCity" type="string" column="appl_city" length="32" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="applAddress" type="string" column="appl_address" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="applIp" type="string" column="appl_ip" length="16" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="applAddtime" type="integer" column="appl_addtime" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
